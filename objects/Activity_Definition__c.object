    <validationRules>
        <fullName>EID_Planned_Start_End_Date_Offset</fullName>
        <active>true</active>
        <description>If Estimated Install Date is selected as the offset criteria, then planned start date offset has to be greater than or equal to planned end date offset.</description>
        <errorConditionFormula>IF( ISPICKVAL( Offset_Criteria__c , &quot;Estimated Install Date&quot;) ,  ((Planned_Start_Date_Offset__c &lt; Planned_End_Date_Offset__c) &amp;&amp; ( ((Planned_Start_Date_Offset__c &lt; 0)&amp;&amp;(Planned_End_Date_Offset__c &lt; 0)) || ((Planned_Start_Date_Offset__c &gt; 0)&amp;&amp;(Planned_End_Date_Offset__c &gt; 0)) ) ) || ((Planned_Start_Date_Offset__c &lt; 0)&amp;&amp;(Planned_End_Date_Offset__c &gt; 0)), FALSE) &amp;&amp; NOT(BEGINS($User.Username,&quot;integration_user@adp.com&quot;))</errorConditionFormula>
        <errorMessage>For offset criteria &quot;Estimated Install Date,&quot; Planned Start Date Offset has to be greater than or equal to Planned End Date Offset.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Active_Task</fullName>
        <active>false</active>
        <errorConditionFormula>AND(Total_Task_Definition_Children__c = 0, Active__c)</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Cannot set as Active if there are no Active Task Definition assign</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_EID_Negative_Planned_Start_End</fullName>
        <active>true</active>
        <description>Only when Offset Criteria is &quot;Estimated Install Date,&quot; can the Planned Start and End Date Offsets be negative.</description>
        <errorConditionFormula>NOT( ISPICKVAL(Offset_Criteria__c, &quot;Estimated Install Date&quot;) ) &amp;&amp; ( ( Planned_Start_Date_Offset__c &lt; 0 ) || ( Planned_End_Date_Offset__c &lt; 0 ) ) &amp;&amp; NOT(BEGINS($User.Username,&quot;integration_user@adp.com&quot;))</errorConditionFormula>
        <errorMessage>Only &quot;Estimated Install Date&quot; Offset Criteria can have negative Planned Date Offsets.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RCD_Planned_Start_End_Date_Offset</fullName>
        <active>true</active>
        <description>If Roll Call Date is selected as the offset criteria, then planned start date offset has to be less than or equal to planned end date offset.</description>
        <errorConditionFormula>IF( ISPICKVAL( Offset_Criteria__c , &quot;Roll Call Date&quot;) , ( Planned_Start_Date_Offset__c &gt; Planned_End_Date_Offset__c ), FALSE) &amp;&amp; NOT(BEGINS($User.Username,&quot;integration_user@adp.com&quot;))</errorConditionFormula>
        <errorMessage>For offset criteria &quot;Roll Call Date,&quot; Planned Start Date Offset has to be less than or equal to Planned End Date Offset</errorMessage>
    </validationRules>
